	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o key_stat_collector.o ../ksniffer/key_stat_collector.cpp

main.o: ../server/main.cpp ../common/ip.hpp \
		../common/dns.hpp \
		../common/dns.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o ../server/main.cpp

server.o: ../server/server.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o server.o ../server/server.cpp

